<script type="68d96e045297dfea22139698-text/javascript">
    var expiration, password, captcha, storagefolder;
    var links = [];
    var uploadedFiles;
    var lastUploadedTime = 0;
    var lastUploadedSize = 0;
    var i = 0;

    $(document).ready(function () {
        
        var dropzone = new Dropzone("#dropzone", {
            url: "https://upload5.easyupload.io/action.php",
            chunking: true,
            forceChunking: true,
            chunkSize: 100000000,
            dictDefaultMessage: "click here or drop files to upload or transfer<br><span>(Max 50 files, 10 GB per file, total 50 GB)</span>",
            retryChunks: true,
            maxFilesize: null,
            maxFiles: null,
            parallelUploads: 3,
            autoProcessQueue: false
        });


        dropzone.on("addedfiles", function (files) {
            // console.log("addedfiles");
            uploadedFiles = files;

            var totalSize = 0;
            if (files.length > 50) {
                $("#upload-error .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + 'Max 50 files are allowed.');
                $("#upload-error").modal("open");
                dropzone.options.autoProcessQueue = false;
                dropzone.removeAllFiles(true);
                return;
            }
            Array.from(uploadedFiles).forEach((file, index) => {
                if (file.size > 10000000000) {
                    $("#upload-error .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + 'File is too big. Max 10 GiB is allowed.');
                    $("#upload-error").modal("open");
                    dropzone.options.autoProcessQueue = false;
                    dropzone.removeAllFiles(true);
                    totalSize = -1;
                    return;
                }
                totalSize += file.size;
            });
            if (totalSize > 50000000000) {
                $("#upload-error .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + 'Total file size is too big. Max 250 GiB is allowed.');
                $("#upload-error").modal("open");
                dropzone.options.autoProcessQueue = false;
                dropzone.removeAllFiles(true);
                return;
            } else if (totalSize < 0) {
                return;
            }
            if (totalSize < 52428800) {
                dropzone.options.parallelUploads = 10;
            }

            $("#expiration").selectpicker("val", "7").selectpicker('refresh');
            $("#password").val("");

            var instance = M.Modal.getInstance($("#upload-modal"));
            instance.options.onCloseStart = function () {
                // console.log("onCloseStart");
                dropzone.removeAllFiles(true);
                $('.dz-stats').hide();
            };
            instance.open();

            $('.dz-remaining-time').text('');
            $('.dz-total-progress').text('');
            $('.dz-upload-speed').text('');
            $('.dz-stats').show();
        });


        dropzone.on("success", function (file, response) {
            console.log("success");

            var r = JSON.parse(file.xhr.response);
            // console.log(r);

            if (r.status) {
                var audio = new Audio("https://easyupload.io/library/completed.mp3");
                audio.play();
                links.push(r.download_link);

                if (dropzone.files.length == 1) {
                    completeSingleUpload();
                    this.options.autoProcessQueue = false;

                    var mail = $('#share-mail1').val();
                        if (mail.length > 2) {
                            $.ajax({
                                type: "POST",
                                url: "https://easyupload.io/action.php",
                                data: {
                                    type: "mail",
                                    captcha: 'keo',
                                    link: $("#upload-link").val(),
                                    mail1: $("#share-mail1").val(),
                                    yourname: $("#share-yourname").val(),
                                    yourmessage: $("#share-yourmessage").val()
                                },
                                dataType: "json",
                                success: function (r) {
                                    if (r.status) {
                                        $("#share .error").html("<img class='icon' src='https://easyupload.io/img/check.png' alt='upload'/>" + r.data);
                                        window.location.href = 'https://easyupload.io/links?id='+$("#upload-link").val();
                                    } else {
                                        $("#share .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + r.data);
                                        window.location.href = 'https://easyupload.io/links?id='+$("#upload-link").val();
                                    }
                                },
                                error: function (r) {
                                    console.log(r);
                                    window.location.href = 'https://easyupload.io/links?id='+$("#upload-link").val();
                                }
                            });
                        } else {
                          window.location.href = 'https://easyupload.io/links?id='+$("#upload-link").val();
                        }
                } else if (dropzone.files.length == links.length) {
                    completeMultipleUpload();
                    this.options.autoProcessQueue = false;
                }

            } else {
                $("#upload-error .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + r.data);
                $("#upload-error").modal("open");
                this.options.autoProcessQueue = false;
            }
        });

        function completeSingleUpload() {
            $('.dz-stats').hide();

            $(".upload-link").val(links[0]);
            // $(".dropzone").addClass("hide");
            // $(".upload-section .upload-success").removeClass("hide");

            links = [];
        }

        function completeMultipleUpload() {
            console.log("completeMultipleUpload");

            $.ajax({
                type: "POST",
                url: "https://easyupload.io/action.php",
                data: {
                    type: "multiple-completed",
                    captcha: captcha
                },
                dataType: "json",
                success: function (r) {
                    if (r.status) {
                        $('.dz-stats').hide();

                        $(".upload-link").val(r.download_link);
                        var mylink = r.download_link;
                        // $(".dropzone").addClass("hide");
                        // $(".upload-section .upload-success").removeClass("hide");

                        links = [];
                        window.scrollTo(0, 0);
                        var mail = $('#share-mail1').val();
                        if (mail.length > 2) {
                            $.ajax({
                                type: "POST",
                                url: "https://easyupload.io/action.php",
                                data: {
                                    type: "mail",
                                    captcha: 'keo',
                                    link: $("#upload-link").val(),
                                    mail1: $("#share-mail1").val(),
                                    yourname: $("#share-yourname").val(),
                                    yourmessage: $("#share-yourmessage").val()
                                },
                                dataType: "json",
                                success: function (r) {
                                    if (r.status) {
                                        $("#share .error").html("<img class='icon' src='https://easyupload.io/img/check.png' alt='upload'/>" + r.data);
                                        window.location.href = 'https://easyupload.io/links?id='+mylink;
                                    } else {
                                        $("#share .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + r.data);
                                        window.location.href = 'https://easyupload.io/links?id='+mylink;
                                    }
                                },
                                error: function (r) {
                                    console.log(r);
                                    window.location.href = 'https://easyupload.io/links?id='+mylink;
                                }
                            });
                        } else {
                          window.location.href = 'https://easyupload.io/links?id='+r.download_link;
                        }
                    } else {
                        $("#share .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + r.data);
                    }
                },
                error: function (r) {
                    console.log(r);
                }
            });
        }


        $("#upload-modal #upload").click(function () {
            var instance = M.Modal.getInstance($("#upload-modal"));
            instance.options.onCloseStart = null;
            instance.close();

            grecaptcha.execute('6LfWajMdAAAAAGLXz_nxz2tHnuqa-abQqC97DIZ3', {action: 'validate_upload'})
                .then(function (token) {
                    // add token value to form
                    uploadValidate(token);
                });
        });


        function uploadValidate(jeton) {
            var files = [];
            Array.from(uploadedFiles).forEach((file, index) => {
                files.push(file.upload.uuid);
            });

            $.ajax({
                type: "POST",
                url: "https://easyupload.io/action.php",
                data: {
                    type: "validate_tuf",
                    captcha: jeton,
                    files: files
                },
                dataType: "json",
                success: function (r) {
                    if (r.status) {
                        expiration = $("#expiration").val();
                        storagefolder = $("#storagefolder").val();
                        password = $("#password").val();
                        captcha = jeton;

                        links = [];
                        dropzone.processQueue();
                    } else {
                        $("#share .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + r.data);
                    }
                },
                error: function (r) {
                    console.log(r);
                }
            });
        }

        window.uploadValidate = uploadValidate;


        dropzone.on("sending", function (file, xhr, formData) {
            // console.log("sending");

            formData.append("type", "dropzone-multi-upload");
            formData.append("expiration", expiration);
            formData.append("storagefolder", storagefolder);
            formData.append("password", password);
            formData.append("captcha", captcha);

            this.options.autoProcessQueue = true;
        });

        dropzone.on("uploadprogress", function (file, progress, bytesSent) {
            if (file.previewElement) {
                var totalSize = file.size;
                var uploadedSize = file.upload.bytesSent;
                if (uploadedSize > totalSize) {
                    uploadedSize = totalSize;
                }
                var progress = (uploadedSize * 100 / totalSize).toFixed(2);

                var el = file.previewElement;
                $(el).find('span[data-dz-uploadprogress]').css('width', progress + '%');
                $(el).find('div[data-dz-uploadpercentage]').text(progress + '%');

                if (progress >=100) {
                  $(el).find('div[data-dz-uploadpercentage]').text('Success!. It may take up to a minute to get the link. Please wait.');
                }
            }

            if (lastUploadedTime == 0) {
                lastUploadedTime = Date.now();
            }

            var totalSize = 0;
            var uploadedSize = 0;
            Array.from(uploadedFiles).forEach((file, index) => {
                totalSize += file.size;
                uploadedSize += file.upload.bytesSent;
            });
            if (uploadedSize > totalSize) {
                uploadedSize = totalSize;
            }
            var progress = (uploadedSize * 100 / totalSize).toFixed(2);

            $('.dz-total-progress').text((uploadedSize * 100 / totalSize).toFixed(2) + "%");

            i += 1;

            if (i % 15 == 0) {
                calculateSpeed(uploadedSize, totalSize);
                i = 0;
            }
        });

        function calculateSpeed(uploadedSize, totalSize) {
            var now = Date.now();

            var differenceSize = uploadedSize - lastUploadedSize;
            if (differenceSize < 0) {
                differenceSize = 0;
            }

            var differenceTime = now - lastUploadedTime;
            if (differenceTime > 0) {
                var speed = differenceSize * 1000 / differenceTime;
                $('.dz-upload-speed').text(getSpeed(speed * 8));
            }

            if (speed > 0) {
                var remainingSize = totalSize - uploadedSize;
                var remainingTime = Math.round(remainingSize / speed);

                $('.dz-remaining-time').text(getTime(remainingTime));
            }

            lastUploadedSize = uploadedSize;
            lastUploadedTime = now;
        }

        function getTime(second) {
            var hour = 0;
            var minute = 0;

            while (second >= 60) {
                minute += 1;
                second -= 60;
            }
            while (minute >= 60) {
                hour += 1;
                minute -= 60;
            }

            if (hour < 10) {
                hour = '0' + hour;
            }
            if (minute < 10) {
                minute = '0' + minute;
            }
            if (second < 10) {
                second = '0' + second;
            }
            return hour + ':' + minute + ':' + second;
        }

        function getSpeed(speed) {
            if (speed >= 1099511627776) {
                speed = (speed / 1099511627776).toFixed(2);
                return speed + " Tbit/Sec";
            } else if (speed >= 1073741824) {
                speed = (speed / 1073741824).toFixed(2);
                return speed + " Gbit/Sec";
            } else if (speed >= 1048576) {
                speed = (speed / 1048576).toFixed(2);
                return speed + " Mbit/Sec";
            } else if (speed >= 1024) {
                speed = (speed / 1024).toFixed(2);
                return speed + " Kbit/Sec";
            }
            return speed + " bit/Sec";
        }


        dropzone.on("error", function (file, message) {
            console.log("error: " + message);

            $("#upload-error .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + message);
            $("#upload-error").modal("open");

            this.options.autoProcessQueue = false;
        });


        $(".upload-link").click(function () {
            this.select();
        });


        $('#share-checkbox').change(function () {
            var checked = $(this).prop('checked');

            if (checked) {
                $('#upload-modal .share-div').removeClass('hidden');
            } else {
                $('#upload-modal .share-div').addClass('hidden');
            }
        });


        var clipboard = new ClipboardJS('.upload-success .icon');
        clipboard.on('success', function (e) {
            M.toast({html: 'Link Copied!', classes: 'rounded'});
        });
        clipboard.on('error', function (e) {
            console.log(e);
        });

        $("#send").click(function () {
            grecaptcha.execute('6LfWajMdAAAAAGLXz_nxz2tHnuqa-abQqC97DIZ3', {action: 'validate_share'})
                .then(function (token) {
                    // add token value to form
                    share(token);
                });
        });


        function share(jeton) {
            $.ajax({
                type: "POST",
                url: "https://easyupload.io/action.php",
                data: {
                    type: "mail",
                    captcha: jeton,
                    link: $("#upload-link").val(),
                    mail1: $("#mail1").val(),
                    yourname: $("#yourname").val(),
                    yourmessage: $("#yourmessage").val()
                },
                dataType: "json",
                success: function (r) {
                    if (r.status) {
                        $("#share .error").html("<img class='icon' src='https://easyupload.io/img/check.png' alt='upload'/>" + r.data);
                        var instance = M.Modal.getInstance($("#share"));
                        instance.options.onCloseStart = null;
                        instance.close();
                        M.toast({html: 'Mail Sent', classes: 'rounded'});
                    } else {
                        $("#share .error").html("<img class='icon' src='https://easyupload.io/img/warning.png' alt='upload'/>" + r.data);
                    }
                },
                error: function (r) {
                    console.log(r);
                }
            });
        }

        window.share = share;

        var switchToPatreon = 0;

        if (switchToPatreon) {
            window.setTimeout(function () {
                var switchToPatreonModal = M.Modal.getInstance($("#switchToPatreon"));
                switchToPatreonModal.open();
                setCookie('switchToPatreons1', '1', 1);
            }, 500);
        }
    });


    function setCookie(name, value, days) {
        var expires = "";
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + (value || "") + expires + "; path=/";
    }
</script>
